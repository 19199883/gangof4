		<!-- open_volume_check -->
		<open_volume_check>
			<!-- 0:off; 1:on; -->
			<switch>1</switch>
			<speculate>1199</speculate>
			<arbitrage>9999</arbitrage>
			<total>9999</total>
		</open_volume_check>

		<cancel_time_check>
			<!-- 0:off; 1:on; -->
			<switch>1</switch>
			<init_cancel_times_at_start>1</init_cancel_times_at_start>
			<warn_threshold>480</warn_threshold>
			<upper_limit>490</upper_limit>
		</cancel_time_check>
